{lib_dirs, ["deps"]}.

{pre_hooks, [
    {compile, "make v8"},
    {clean, "make local-clean"},
    {'delete-deps', "make local-clean-all"}
]}.

{deps, [
    {'jiffy', ".*", {git, "git://github.com/davisp/jiffy.git", "master"}},
    {'poolboy', "0.*", {git, "https://github.com/devinus/poolboy.git", "master"}}
]}.
